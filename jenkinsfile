pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                git 'https://github.com/ansh8898/Contact-Management-System.git'
            }
        }

        stage('Build') {
            steps {
                sh 'mvn clean install'
            }
        }

        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
    }
}
