pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                script {
                    // Set the path to the Git executable
                    env.PATH = "${tool 'Git'}/bin:${env.PATH}"
                    
                    // Checkout code from Git repository
                    checkout([$class: 'GitSCM', 
                              branches: [[name: '*/main']],
                              doGenerateSubmoduleConfigurations: false,
                              extensions: [],
                              userRemoteConfigs: [[url: 'https://github.com/ansh8898/Contact-Management-System']]])
                }
            }
        }
        // Add more stages as needed
    }

    // Post-build actions
    post {
        always {
            // Cleanup steps or notifications
        }
    }
}
